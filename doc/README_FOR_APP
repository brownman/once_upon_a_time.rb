function set_materials()
{
  var material = new THREE.ParticleCanvasMaterial(
      {

color: 0xffffff,
program: function ( context )
{

context.beginPath();
context.arc( 0, 0, 1, 0, PI2, true );
context.closePath();
context.fill();

}//program..

}//var material 

